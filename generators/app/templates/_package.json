{
  "name": "<%= app %>",
  "description": "<%= projectDescription %>",
  "homepage": "<%= homepage %>",
  "author": {
    "name": "<%= username %>",
    "email": "<%= userEmail %>",
    "url": "<%= authorUrl %>"
  },
  "private": true,
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "rimraf": "rimraf",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "webpack": "webpack --progress --profile --bail",
    "webpack-dev-server": "webpack-dev-server",
    "webdriver-manager": "webdriver-manager",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist compiled dll",
    "clean:dist": "npm run rimraf -- dist",
    "clean:dll": "npm run rimraf -- dll",
    "preclean:install": "npm run clean",
    "clean:install": "npm set progress=false && npm install",
    "preclean:start": "npm run clean",
    "clean:start": "npm start",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "build": "npm run build:dev",
    "prebuild:dev": "npm run clean:dist",
    "build:dev": " npm run clean:dist && npm run webpack -- --config config/webpack.dev.js",
    "prebuild:prod": "npm run clean:dist",
    "build:prod": "npm run clean:dist && webpack --config config/webpack.prod.js  --progress --profile",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --open --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "server:test": "http-server dist-demo -c-1 --cors",
    "server:prod:ci": "http-server dist -p 3000 -c-1 --cors",
    "webdriver:update": "npm run webdriver-manager update",
    "webdriver:start": "npm run webdriver-manager start",
    "lint": "npm run tslint \"src/**/*.ts\" --force",
    "pree2e": "npm run webdriver:update -- --standalone",
    "pretest": "npm run lint",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "start": "npm run server:dev",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "license": "MIT",
  "dependencies": {
    "animate.css": "^3.5.2",
    "bootstrap": "^3.3.7",
    "bootstrap-sass": "^3.3.7",
    "core-js": "2.4.1",
    "jquery": "^3.2.1",
    "lodash": "4.17.4",
    "rxjs": "5.1.1",
    "tether": "1.4.0",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@types/jquery ": "2.0.40",
    "@types/lodash": "4.14.52",
    "@types/node": "6.0.63",
    "@types/source-map": "0.5.0",
    "@types/uglify-js": "2.6.28",
    "@types/webpack": "2.2.5",
    "add-asset-html-webpack-plugin": "1.0.2",
    "assets-webpack-plugin": "3.5.1",
    "awesome-typescript-loader": "3.0.3",
    "compression-webpack-plugin": "0.3.2",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "es6-promise": "4.0.5",
    "es6-shim": "0.35.3",
    "es7-reflect-metadata": "1.6.0",
    "eslint": "^3.18.0",
    "eslint-config-defaults": "^9.0.0",
    "eslint-config-xo-space": "^0.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.6.0",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.2",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "file-loader": "0.10.0",
    "find-root": "1.0.0",
    "html-webpack-plugin": "2.28.0",
    "html-webpack-pug-plugin": "0.0.3",
    "imports-loader": "^0.7.0",
    "jest": "^19.0.2",
    "jest-cli": "^19.0.2",
    "json-loader": "0.5.4",
    "node-sass": "4.5.0",
    "npm-run-all": "4.0.1",
    "optimize-js-plugin": "0.0.4",
    "parse5": "3.0.1",
    "postcss-loader": "1.2.2",
    "pug": "2.0.0-beta3",
    "pug-html-loader": "1.1.0",
    "pug-loader": "2.3.0",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "1.6.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.1.1",
    "script-ext-html-webpack-plugin": "1.7.1",
    "source-map-loader": "0.1.6",
    "string-replace-loader": "1.0.5",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "ts-node": "2.1.0",
    "tslib": "1.5.0",
    "tslint": "4.4.2",
    "typedoc": "0.5.5",
    "typescript": "2.1.6",
    "url-loader": "0.5.7",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.0",
    "webpack-dev-server": "2.3.0",
    "webpack-dll-bundles-plugin": "1.0.0-beta.5",
    "webpack-merge": "2.6.1"
  }
}
